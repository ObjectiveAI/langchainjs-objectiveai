{
  "name": "@objectiveai/langchain",
  "version": "0.1.2",
  "description": "Objective AI integrations for LangChain.js",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ObjectiveAI/langchainjs-objectiveai.git"
  },
  "packageManager": "yarn@3.5.1",
  "homepage": "https://github.com/ObjectiveAI/langchainjs-objectiveai",
  "scripts": {
    "turbo:command": "turbo",
    "build": "yarn turbo:command build:internal",
    "build:internal": "yarn lc_build --create-entrypoints --pre --tree-shaking",
    "lint:eslint": "NODE_OPTIONS=--max-old-space-size=4096 eslint --cache --ext .ts,.js src/",
    "lint:dpdm": "dpdm --skip-dynamic-imports circular --exit-code circular:1 --no-warning --no-tree src/*.ts src/**/*.ts",
    "lint": "yarn lint:eslint && yarn lint:dpdm",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:dpdm",
    "clean": "rm -rf .turbo dist/",
    "prepack": "yarn build",
    "format": "prettier --config .prettierrc --write \"src\"",
    "format:check": "prettier --config .prettierrc --check \"src\""
  },
  "author": {
    "name": "Objective AI",
    "email": "support@objective-ai.io",
    "url": "https://objective-ai.io"
  },
  "license": "MIT",
  "dependencies": {
    "objectiveai": "1.0.1",
    "openai": "5.12.2",
    "zod": "^3.25.32"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.3.68 <0.4.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@langchain/core": ">=0.3.68 <0.4.0",
    "@langchain/scripts": ">=0.1.0 <0.2.0",
    "@swc/core": "^1.3.90",
    "@swc/jest": "^0.2.29",
    "@tsconfig/recommended": "^1.0.2",
    "dpdm": "^3.14.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.6.4",
    "prettier": "^2.8.3",
    "release-it": "^18.1.2",
    "ts-jest": "^29.1.0",
    "turbo": "^2.5.8",
    "typescript": "~5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "llm",
    "ai",
    "chain",
    "prompt",
    "prompt engineering",
    "machine learning",
    "ml",
    "objectiveai",
    "objective ai",
    "objective",
    "confidence",
    "confidence id",
    "confidence score"
  ],
  "exports": {
    ".": {
      "types": {
        "import": "./index.d.ts",
        "require": "./index.d.cts",
        "default": "./index.d.ts"
      },
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "index.cjs",
    "index.js",
    "index.d.ts",
    "index.d.cts"
  ]
}
